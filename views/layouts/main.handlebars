                        } else {
                            this.message = "Enter " + $('label[for="' + $(this.element).attr('id') + '"]').html().replace(':', '');
                        }
                        if(this.message.indexOf("*")>-1)
                            {this.message= this.message.replace('*', '');}
                        console.log((this.element));
                    });
                    this.defaultShowErrors();
                }
            }
            );
